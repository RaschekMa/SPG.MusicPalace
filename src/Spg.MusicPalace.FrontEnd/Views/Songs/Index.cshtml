@model IEnumerable<Spg.MusicPalace.Dtos.SongDto>

<h3>Liste aller Songs</h3>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Artist</th>
            <th>Album</th>
        </tr>
    </thead>
    <tbody>
        @foreach(Spg.MusicPalace.Dtos.SongDto item in @Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.ArtistName</td>
                <td>@item.AlbumName</td>
            </tr>
        }
    </tbody>
</table>